local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Tempus Hub", "DarkTheme")
_G.Antimod = true
_G.CompassNumber = 1

if game.PlaceId == 3237168 then

